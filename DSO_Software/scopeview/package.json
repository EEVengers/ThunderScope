  "version": "0.1.0",
  "main": "public/electron.tsx",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "3.1.2",
    "electron-is-dev": "^1.1.0",
    "typescript": "3.6.3"
